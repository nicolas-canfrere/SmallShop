{
    "authors": [
        {
            "name": "Nicolas Canfrere",
            "email": "nico.canfrere@gmail.com"
        }
    ],
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "behat/transliterator": "^1.2",
        "doctrine/common": "^2.10",
        "doctrine/doctrine-bundle": "^1.6.10",
        "doctrine/doctrine-migrations-bundle": "^1.3|^2.0",
        "doctrine/orm": "^2.5.11",
        "easycorp/easy-log-handler": "^1.0.7",
        "knplabs/knp-paginator-bundle": "^2.8",
        "league/tactician-bundle": "^1.1",
        "moneyphp/money": "^3.2",
        "myclabs/php-enum": "^1.6",
        "ramsey/uuid": "^3.8",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "4.2.*",
        "symfony/debug-bundle": "*",
        "symfony/dotenv": "4.2.*",
        "symfony/flex": "^1.1",
        "symfony/form": "4.2.*",
        "symfony/framework-bundle": "4.2.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/monolog-bundle": "^3.0",
        "symfony/property-access": "4.2.*",
        "symfony/property-info": "4.2.*",
        "symfony/security-bundle": "4.2.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/stopwatch": "*",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "4.2.*",
        "symfony/translation": "4.2.*",
        "symfony/twig-bundle": "*",
        "symfony/validator": "4.2.*",
        "symfony/var-dumper": "*",
        "symfony/web-profiler-bundle": "*",
        "symfony/yaml": "4.2.*",
        "twig/twig": "^2.6",
        "webmozart/assert": "^1.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Application\\": "src/Application/",
            "Domain\\": "src/Domain",
            "Bundles\\": "src/Bundles/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "phpcs": "phpcs --config-set installed_paths vendor/escapestudios/symfony2-coding-standard"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.2.*"
        }
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "escapestudios/symfony2-coding-standard": "3.5.2",
        "friendsofphp/php-cs-fixer": "^2.14",
        "fzaninotto/faker": "^1.8",
        "maglnet/composer-require-checker": "^1.1",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.0",
        "sensiolabs/security-checker": "^5.0",
        "squizlabs/php_codesniffer": "3.4.0"
    }
}
